        </activity>
        <activity android:name=".activity.poetry_hall.TranslationActivity"></activity>
        <activity android:name=".activity.poetry_hall.SearchActivity"></activity>
        <activity android:name=".activity.poetry_hall.PoeterActivity"></activity>
